<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    <url>
        <loc>https://<%= site.hostname %></loc>
    </url>

    <% for (let i=0; i < pastes.length; i++) { %>
        <url>
            <loc>https://<%= site.hostname %>/p/<%= pastes[i].id %>/</loc>
        </url>
    <% } %>
</urlset>